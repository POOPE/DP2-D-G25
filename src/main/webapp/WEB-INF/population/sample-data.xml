	</bean>
	
	<bean id="duty05" class="acme.entities.duties.Duty">
		<property name="officer" ref="officerOfficer" />
		<property name="title" value="A duty" />
		<property name="description" value="Non-finished and public" />
		<property name="executionStart" value="2021/12/12 14:46" />
		<property name="executionEnd" value="2021/12/15 14:46" />
		<property name="workload" value="04:21" />
		<property name="isPublic" value="true" />
		<property name="link" value="" />
	</bean>
	
	<bean id="duty06" class="acme.entities.duties.Duty">
		<property name="officer" ref="officerOfficer1" />
		<property name="title" value="A duty" />
		<property name="description" value="Finished and public" />
		<property name="executionStart" value="2021/12/02 14:46" />
		<property name="executionEnd" value="2021/12/03 14:46" />
