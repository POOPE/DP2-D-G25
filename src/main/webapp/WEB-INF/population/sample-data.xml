	</bean>

	<!-- endeavour -->

	<bean id="endeavour01" class="acme.entities.endeavours.Endeavour">
		<property name="officer" ref="officerOfficer" />
		<property name="executionStart" value="2021/12/01 08:00" />
		<property name="executionEnd" value="2021/12/30 17:00" />
		<property name="duration" value="06:22" />
		<property name="duties">
			<list>
				<ref bean="duty05" />
				<ref bean="duty06" />
			</list>
		</property>
		<property name="isPublic" value="true" />
	</bean>
	
	<bean id="endeavour02" class="acme.entities.endeavours.Endeavour">
		<property name="officer" ref="officerOfficer" />
		<property name="executionStart" value="2020/10/01 14:46" />
		<property name="executionEnd" value="2022/10/02 14:46" />
		<property name="duration" value="36:32" />
		<property name="duties">
			<list>
				<ref bean="duty04" />
				<ref bean="duty03" />
			</list>
		</property>
		<property name="isPublic" value="false" />
	</bean>
	
	<bean id="endeavour03" class="acme.entities.endeavours.Endeavour">
		<property name="officer" ref="officerOfficer" />
		<property name="executionStart" value="2020/10/01 11:22" />
		<property name="executionEnd" value="2022/10/02 22:33" />
		<property name="duration" value="36:32" />
		<property name="duties">
			<list>
				<ref bean="duty04" />
			</list>
		</property>
		<property name="isPublic" value="false" />
	</bean>
	
	<bean id="endeavour04" class="acme.entities.endeavours.Endeavour">
		<property name="officer" ref="officerOfficer" />
		<property name="executionStart" value="2021/12/01 14:00" />
